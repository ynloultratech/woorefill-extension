{
  "name": "ynloultratech/woorefill-extension",
  "description": "WooRefill is a extension for WooCommerce to add wireless plans to your shop and make refills.",
  "type": "wordpress-plugin",
  "keywords": ["wordpress", "plugin", "refill"],
  "homepage": "https://github.com/ynloultratech/woorefill-extension",
  "license": "Apache.20",
  "authors": [
    {
      "name": "YnloUltratech",
      "email": "support@ynloultratech.com",
      "homepage": "https://github.com/ynloultratech"
    }
  ],
   "autoload": {
    "psr-4": {
      "WooRefill\\": "src/",
      "WooRefill\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.3",
    "twig/twig": "^1.28",
    "symfony/form": "2.7.*",
    "symfony/http-foundation": "2.7.*",
    "symfony/dependency-injection": "2.7.*",
    "symfony/config": "2.7.*",
    "symfony/twig-bridge": "2.7.*",
    "symfony/filesystem": "2.7.*",
    "symfony/event-dispatcher": "2.7.*",
    "symfony/intl": "2.7.*",
    "symfony/property-access": "2.7.*",
    "symfony/finder": "2.7.*",
    "guzzlehttp/guzzle": "^3.8",
    "erusev/parsedown": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "scripts": {
    "post-install-cmd": [
      "WooRefill\\SandBox\\ScriptHandler::sandbox"
    ],
    "post-update-cmd": [
      "WooRefill\\SandBox\\ScriptHandler::sandbox"
    ]
  },
  "extra": {
    "sandbox": {
      "prefix": "WooRefill",
      "name": [
        "*.php",
        "*.yml",
        "*.json",
        "*.xml"
      ],
      "clean": {
        "files": [
        ],
        "dirs": [
          ".git",
          "tests",
          "test",
          "Test",
          "Tests",
          "intl/Resources/data",
          "guzzlehttp/guzzle/docs"
        ],
        "exclude":[
          "twig/twig/lib/Twig/Node/Expression/Test"
        ]
      }
    }
  }
}
